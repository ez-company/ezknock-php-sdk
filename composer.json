{
    "name": "ezknock/ezknock-php-sdk",
    "description": "EZ Knock API client built on top of HTTPlug",
    "keywords": [
        "api",
        "guzzle",
        "ezknock",
        "ezknockmarketplace.com"
    ],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "EZ Knock Team",
            "homepage": "https://www.ezknockmarketplace.com"
        },
        {
            "name": "Jovanni Lo",
            "email": "lodev09@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/ez-company/ezknock-php-sdk/issues",
        "source": "https://github.com/ez-company/ezknock-php-sdk"
    },
    "autoload": {
        "psr-4": {
            "EZKnock\\": [
                "src"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EZKnock\\Test\\": [
                "tests"
            ]
        }
    },
    "require": {
        "php": ">= 7.3",
        "ext-json": "*",
        "php-http/client-common": "^1.9 || ^2.0",
        "php-http/client-implementation": "*",
        "php-http/discovery": "^1.4",
        "php-http/httplug": "^1.0 || ^2.0",
        "php-http/message": "^1.7",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "guzzlehttp/psr7": "^1.7.0",
        "php-http/mock-client": "^1.4",
        "phpunit/phpunit": "^7.5 || ^8.5 || ^9.3",
        "squizlabs/php_codesniffer": "^3.1"
    },
    "config": {
        "sort-packages": true
    }
}
